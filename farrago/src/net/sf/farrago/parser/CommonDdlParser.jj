}

String NonReservedKeyWord() :
{
    String kw;
}
{
    (
        kw = CommonNonReservedKeyWord()
        {
            return kw;
        }
        |
        kw = DdlNonReservedKeyWord()
        {
            return kw;
        }
    )
}

/**
 * Parses a non-reserved DDL keyword for use as an identifier.
 */
String DdlNonReservedKeyWord() :
{
}
{
    (
        <CASCADE>
        | <CATALOG>
        | <DATA>
        | <INDEX>
        | <JAVA>
        | <KEY>
        | <LIBRARY>
        | <OPTIONS>
        | <PRESERVE>
        | <RESTRICT>
        | <SCHEMA>
        | <SERVER>
        | <TEMPORARY>
        | <VERSION>
        | <VIEW>
        | <WRAPPER>
    )
        {
            return getToken(0).image.toUpperCase();
        }
}

