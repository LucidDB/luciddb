#!/bin/sh
# $Id$
# Run the sqlline command-line SQL interpreter as a client
# to a Farrago server

BIN_DIR=$(cd `dirname $0`; pwd)

source $BIN_DIR/defineFarragoRuntime.sh

java ${JAVA_ARGS} ${SQLLINE_JAVA_ARGS} \
    -u jdbc:farrago:rmi://localhost \
    -d net.sf.farrago.jdbc.client.FarragoJdbcClientDriver \
    -n guest $*
